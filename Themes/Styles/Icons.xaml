<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Dialog Icons -->
    <BitmapImage x:Key="IconInfo" UriSource="pack://application:,,,/Resources/info01.png"/>
    <BitmapImage x:Key="IconWarning" UriSource="pack://application:,,,/Resources/warning01.png"/>
    <BitmapImage x:Key="IconSuccess" UriSource="pack://application:,,,/Resources/checked01.png"/>
    <BitmapImage x:Key="IconError" UriSource="pack://application:,,,/Resources/cancel01.png"/>

    <!-- Tab Icons -->
    <BitmapImage x:Key="IconFileContent" UriSource="pack://application:,,,/Resources/filecontent01.png"/>
    <BitmapImage x:Key="IconTreeStructure" UriSource="pack://application:,,,/Resources/treestructure01.png"/>
    <BitmapImage x:Key="IconFolderContent" UriSource="pack://application:,,,/Resources/foldercontent01.png"/>
    <BitmapImage x:Key="IconSettings" UriSource="pack://application:,,,/Resources/settings01.png"/>

    <!-- Toolbar/Action Icons -->
    <BitmapImage x:Key="IconSave" UriSource="pack://application:,,,/Resources/save01.png"/>
    <BitmapImage x:Key="IconDelete" UriSource="pack://application:,,,/Resources/delete01.png"/>
    <BitmapImage x:Key="IconUndo" UriSource="pack://application:,,,/Resources/arrow01.png"/>
    <BitmapImage x:Key="IconRedo" UriSource="pack://application:,,,/Resources/arrow02.png"/>

</ResourceDictionary>
